{
  "name": "dock108home",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,yml}\"",
    "postbuild": "next-sitemap",
    "type-check": "tsc --noEmit",
