  "private": true,
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:docs": "npm run build && node --loader ts-node/esm scripts/check-doc-links.ts",
    "check-links": "node --loader ts-node/esm scripts/check-doc-links.ts",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,yml}\"",
    "postbuild": "next-sitemap",
    "type-check": "tsc --noEmit",
    "test": "jest --detectOpenHandles",
    "test:e2e": "cross-env NODE_ENV=test playwright test",
    "test:e2e:oauth": "cross-env NODE_ENV=test playwright test tests/e2e/stripe-oauth.test.ts",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "vercel:env": "ts-node scripts/set-vercel-env.ts",
    "pw-test": "playwright test",
    "start-preview": "next start -p 5555",
    "lh-ci": "lhci autorun",
    "size:check": "node scripts/size-check.js"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md,mdx,yml}": [
      "prettier --write"
    ],
    "{app,components,lib,pages}/**/*.{js,jsx,ts,tsx}": [
      "next lint --fix"
    ]
